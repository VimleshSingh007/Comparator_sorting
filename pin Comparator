public class pin_comparator implements Comparator<student>{
    
    public int compare(student s1,student s2){
       Integer  p1=s1.getPin();
       Integer  p2=s2.getPin();
       
       if(p1>p2){
           return -1;
       }
       else if(p1<p2){
           return 1;
       }
       else
           return 0;
    }
    
}
